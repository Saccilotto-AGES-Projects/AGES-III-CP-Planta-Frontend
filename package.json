{
  "name": "cpplanta-frontend",
  "author": "AGES",
  "description": "Frontend for CPPlanta project",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript",
    "tailwindcss",
    "eslint",
    "prettier",
    "lint-staged",
    "commitizen",
    "semantic-release",
    "jest"
  ],
  "private": true,
  "repository": {
    "type": "gitlab",
    "url": "https://tools.ages.pucrs.br/cp-planta/backend"
  },
  "bugs": {
    "url": "https://tools.ages.pucrs.br/cp-planta/backend/-/issues"
  },
  "homepage": "https://www.ages.pucrs.br/projetos-2/cpplanta/",
  "scripts": {
    "start": "next start",
    "start:prod": "next build && next start",
    "dev": "next dev",
    "build": "next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --passWithNoTests",
    "lint": "eslint src --max-warnings=0",
    "prettier": "npx prettier --write --ignore-unknown .",
    "format": "npm run prettier",
    "commit": "git-cz",
    "linters": "npm run lint && npm run prettier",
    "release": "semantic-release",
    "commitlint": "commitlint --edit"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "pretty-quick --staged",
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^2.0.4",
    "aos": "^3.0.0-beta.6",
    "axios": "^1.7.7",
    "cpplanta-frontend": "file:",
